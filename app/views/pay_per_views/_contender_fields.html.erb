<div class="nested-fields">
  <div class="field">
    
    <%= f.hidden_field :type, value: f.object.class.name %>

    <%= f.label :name, "#{f.object.class.name.humanize} has Name" %>
    <%= f.select :name, f.object.possible_contenders, include_blank: true %>


    <%= link_to_remove_association "remove contender", f %>
  </div>
</div>
